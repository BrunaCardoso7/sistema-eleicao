{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "env": [
        {
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_KEY": "@supabase_key"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
  